{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "version": 2,
  "builds": [
    { "src": "api/create_preferences.js", "use": "@vercel/node" },
    { "src": "vite.config.ts", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}